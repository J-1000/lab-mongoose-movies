<h2>Celebrities</h2>
<ul>
  {{#each celebrityData}}
    <li>
      <a href="/celebrities/{{_id}}">{{name}}</a>
      <form action="/celebrities/{{_id}}/delete" method="POST">
        <button type="submit">Delete</button>
      </form>
    
    </li>

  {{/each}}
<br>
  <a href="/celebrities/new">Add a new Celeb</a>
</ul>
