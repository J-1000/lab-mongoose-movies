<h2>{{title}}</h1>
<p>Welcome to {{title}}</p>

<div id="celebrities">
  {{#with celebrity}}
  <div id="celebrity">
    <p>{{name}}</p>
    <p>{{occupation}}</p>
    <p>{{catchPhrase}}</p>
    <form action="/celebrities/{{_id}}/edit" method="GET">
      <button>Edit celebrity</button>
    </form>
  </div>
  {{/with}}
</div>