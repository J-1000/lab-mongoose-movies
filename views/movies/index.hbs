<h1>Movies</h1>
{{#each movieList}}
  <h2>{{this.title}}</h2>
  <p>{{this.genre}}</p>
  <p>{{this.plot}}</p>
  <h3>Starring:</h3>
  {{#each this.cast}}
    <li>{{name}}</li>
  {{/each}}
  <a href="/movies/{{this._id}}/edit">Edit movie</a>
{{/each}}

<br>

<br>

<a href="/movies/new">Add new movie</a>